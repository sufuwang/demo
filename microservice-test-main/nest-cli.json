{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/microservice-test-main/src",
  "monorepo": true,
  "root": "apps/microservice-test-main",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/microservice-test-main/tsconfig.app.json"
  },
  "projects": {
    "microservice-test-main": {
      "type": "application",
      "root": "apps/microservice-test-main",
      "entryFile": "main",
      "sourceRoot": "apps/microservice-test-main/src",
      "compilerOptions": {
        "tsConfigPath": "apps/microservice-test-main/tsconfig.app.json"
      }
    },
    "app": {
      "type": "application",
      "root": "apps/app",
      "entryFile": "main",
      "sourceRoot": "apps/app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app/tsconfig.app.json"
      }
    },
    "lib": {
      "type": "library",
      "root": "libs/lib",
      "entryFile": "index",
      "sourceRoot": "libs/lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/lib/tsconfig.lib.json"
      }
    },
    "g-rpc": {
      "type": "application",
      "root": "apps/g-rpc",
      "entryFile": "main",
      "sourceRoot": "apps/g-rpc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/g-rpc/tsconfig.app.json",
        "watchAssets": true,
        "assets": ["**/*.proto"],
        "deleteOutDir": true
      }
    }
  }
}